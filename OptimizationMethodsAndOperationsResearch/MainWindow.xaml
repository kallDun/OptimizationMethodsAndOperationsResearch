<Window x:Class="OptimizationMethodsAndOperationsResearch.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OptimizationMethodsAndOperationsResearch"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Frame x:Name="PageFrame" NavigationUIVisibility="Hidden"/>
        <Button Width="35" Height="35" Background="Transparent" BorderBrush="Black" BorderThickness="2"
                HorizontalAlignment="Right" VerticalAlignment="Top" Margin="15"
                Click="RightButton_Click" x:Name="ToRightButton" IsEnabled="False">
            <TextBlock Text=">" FontSize="23" FontFamily="DejaVu Sans"/>
        </Button>
        <Button Width="35" Height="35" Background="Transparent" BorderBrush="Black" BorderThickness="2"
                HorizontalAlignment="Left" VerticalAlignment="Top" Margin="15"
                Click="LeftButton_Click" x:Name="ToLeftButton" IsEnabled="False">
            <TextBlock Text="&lt;" FontSize="23" FontFamily="DejaVu Sans"/>
        </Button>
    </Grid>
</Window>